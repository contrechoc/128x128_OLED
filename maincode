u8g2_uint_t width;      // pixel width of the scrolling text (must be lesser than 128 unless U8G2_16BIT is defined

void setup(void) {

  u8g2.begin();
  u8g2.firstPage();
  u8g2.setFlipMode(1);
  //makeLinePage();

