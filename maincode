void getRandomImageSMALL( ) {

  int h = random(0, 2);
  u8g2.setDrawColor(h);// Black

  int w = random(0, stringLength2);

  u8g2.setDrawColor(1);
u8g2.setBitmapMode(1);
   
  for (int i = 0; i< random(10); i++)
  do {
    u8g2.drawXBMP(random(100),random(100), bicycle_width , bicycle_height, myStrings2[w]);
  } while ( u8g2.nextPage() );

}

void  getRandomImage128x128(   )
{
   int w = random(0, stringLength);

   int h = random(0, 2);
  u8g2.setDrawColor(h);// Black
   
 
  do {
    u8g2.drawXBMP(0,0, testXBM_width , testXBM_height, myStrings[w]);
  } while ( u8g2.nextPage() );
  
}



void ScrollingWithSmallImages() {
 u8g2.setDrawColor(1);
u8g2.setBitmapMode(1);

 int w = random(0, stringLength2);
 int y = random(100);
 for (int i = 0; i< 100; i+=15)
  do {
    u8g2.drawXBMP(i,y-6+random(22), bicycle_width , bicycle_height, myStrings2[w]);
  } while ( u8g2.nextPage() );


}

void getSomwWords() {

  int w = random (0, 3);

  switch (w) {
    case 0:
      do {
        u8g2.setDrawColor(1);
        u8g2.drawBox(0, 0, 128, 128);
        u8g2.setDrawColor(0);
        u8g2.setFont(u8g2_font_7x14B_mr);

        int t = random(7);
        if ( t == 0)
          u8g2.drawStr( 5, 30, "     hype");
        else if (t == 1)
          u8g2.drawStr( 5, 30, "if statement");
        else if (t == 2)
          u8g2.drawStr( 5, 30, "     loop");
        else if (t == 3)
          u8g2.drawStr( 5, 30, " wave wave");
        else if (t == 4)
          u8g2.drawStr( 5, 30, "     trend");
           else if (t == 5)
          u8g2.drawStr( 5, 30, "friend = trend");
            else if (t == 6)
          u8g2.drawStr( 5, 30, "  loveloop");

        t = random(5);
        if ( t == 0)
          u8g2.drawStr( 5, 50, "shopdrop ");
        else if (t == 1)
          u8g2.drawStr( 5, 50, "  dropshop");
        else if (t == 2)
          u8g2.drawStr( 5, 50, "   framefame");
        else if (t == 3)
          u8g2.drawStr( 5, 50, "influence");
        else if (t == 4)
          u8g2.drawStr( 5, 50, "   looklike");

        t = random(5);
        if ( t == 0)
          u8g2.drawStr( 5, 70, "     share");
        else if (t == 1)
          u8g2.drawStr( 5, 70, "  platform");
        else if (t == 2)
          u8g2.drawStr( 5, 70, "     tool");
        else if (t == 3)
          u8g2.drawStr( 5, 70, "  society");
        else if (t == 4)
          u8g2.drawStr( 5, 70, "  together");

        t = random(5);
        if ( t == 0)
          u8g2.drawStr( 5, 90, " succes");
        else if (t == 1)
          u8g2.drawStr( 5, 90, " positive");
        else if (t == 2)
          u8g2.drawStr( 5, 90, "      friend");
        else if (t == 3)
          u8g2.drawStr( 5, 90, " family");
        else if (t == 4)
          u8g2.drawStr( 5, 90, "    hand");

        t = random(5);
        if ( t == 0)
          u8g2.drawStr( 5, 110, "     bliss");
        else if (t == 1)
          u8g2.drawStr( 5, 110, " skill");
        else if (t == 2)
          u8g2.drawStr( 5, 110, "     start");
        else if (t == 3)
          u8g2.drawStr( 5, 110, " new");
        else if (t == 4)
          u8g2.drawStr( 5, 110, "     WOW!");

        // u8g2.setFontDirection(1);
 

      } while ( u8g2.nextPage() );


      break;

    case 1:
      do {
        u8g2.setFont(u8g2_font_t0_13b_me);
        u8g2.setDrawColor(0);
        u8g2.drawBox(0, 0, 128, 128);
        u8g2.setDrawColor(1);

        u8g2.drawStr( 5, 45, "////////////////");
        int t = random(5);
        if ( t == 0)
          u8g2.drawStr( 5, 60, "////galactic////");
        else if (t == 1)
          u8g2.drawStr( 5, 60, "///spaceship////");
        else if (t == 2)
          u8g2.drawStr( 5, 60, "/////stellar////");
        else if (t == 3)
          u8g2.drawStr( 5, 60, "////supernova///");
        else if (t == 4)
          u8g2.drawStr( 5, 60, "////bicycle////");

        u8g2.drawStr( 5, 75, "////////////////");

        for (int i = 0; i < 500; i++)
        {
          u8g2.drawPixel(random(128), random(128));
        }

      } while ( u8g2.nextPage() );



      break;

    case 2:
      do {
        u8g2.setDrawColor(0);
        u8g2.drawBox(0, 0, 128, 128);
        u8g2.setDrawColor(1);
        u8g2.setFont(u8g2_font_inb16_mr);
        u8g2.drawStr( 5, 50, "XXXXX XXXXXX");
        int t = random(5);
        if ( t == 0)
          u8g2.drawStr( 5, 80, "X SPACE X");
        else if (t == 1)
          u8g2.drawStr( 5, 80, "X EARTH X");
        else if (t == 2)
          u8g2.drawStr( 5, 80, "X MARS X");
        else if (t == 3)
          u8g2.drawStr( 5, 80, "X VENUS X");
        else if (t == 4)
          u8g2.drawStr( 5, 80, "X SPACE X");

        u8g2.drawStr( 5, 110, "XXXXXXX XXXX");
      } while ( u8g2.nextPage() );

      break;
    default:
      // statements
      break;
  }

}

void getSomeBiggerPixels()
{
  int w = random (0, 2);

  switch (w) {
    case 0:
      do {
        u8g2.setDrawColor(1);
        u8g2.drawBox(0, 0, 128, 128);
        u8g2.setDrawColor(0);
        for (int i = 0; i < 500; i++)
        {
          u8g2.drawPixel(random(128), random(128));

        }

      } while ( u8g2.nextPage() );

      break;

    case 1:
      do {
        u8g2.setDrawColor(0);
        u8g2.drawBox(0, 0, 128, 128);
        u8g2.setDrawColor(1);
        for (int i = 0; i < 10; i++)
        {
          u8g2.drawBox(int(15 * random(10)), int(15 * random(10)), 10, 10);
        }

      } while ( u8g2.nextPage() );

      break;
    default:
      // statements
      break;
  }
}

void allBigImages(){
   for(  int w =0; w< stringLength; w++)
{
   int h = random(0, 2);
  u8g2.setDrawColor(h);// Black
   
u8g2.setBitmapMode(0);
  do {
    u8g2.drawXBMP(0,0, testXBM_width , testXBM_height, myStrings[w]);
  } while ( u8g2.nextPage() );
delay(1000 + random(3000));
}
}

void loop(void) {
  u8g2.firstPage();

  allBigImages();

  ScrollingWithSmallImages();
  

 if ( random(0,2) == 0)
  getRandomImageSMALL(  );
  else
  getRandomImage128x128( );

  delay(2000 + random(10000));
  
if ( random(0,2) == 0)
  getSomwWords();
    else
  getRandomImage128x128( );

  delay(2000 + random(10000));

if ( random(0,2) == 0)
  makeLinePage();
    else
  getRandomImage128x128( );

  delay(2000 + random(10000));

if ( random(0,2) == 0)
{
  int okPixel = random(0, 3);
  if ( okPixel == 1 )
  {
    makePixel();
  }
}  else
  getRandomImage128x128( );

   delay(2000 + random(10000));

if ( random(0,2) == 0)
{
  int okPixelBIG = random(0, 3);
  if ( okPixelBIG == 1 )
  {
    makePixelBIG();
  }
}  else
  getRandomImage128x128( );

   delay(2000 + random(10000));

  // intergalactic
  // SPACE
  // Andromeda
  // to the Future
  // repeat
  // Spaceship
  // time
  // UNcontrol
  // there are only starts to the left
  // the galaxy cluster wheather is fine

}

void makeLinePage()
{
  int h = random(0, 2);
  u8g2.setDrawColor(h);// Black

  makeLineX(0);
  makeLineX(10);
  makeLineX(20);
  makeLineX(30);
  makeLineX(40);

  makeLineY(0);
  makeLineY(10);
  makeLineY(20);
  makeLineY(30);
  makeLineY(40);

  delay(1000 + random(3000));

}

void makeLineX(int off)
{
  do {

    for (int i = 0; i < 128; i++)
    {
      u8g2.drawPixel(i, 64 + (32 - analogRead(0) / 8) * 2 + off );
    }

  } while ( u8g2.nextPage() );
  delay(random(500));

}

void makeLineY(int off)
{
  do {

    for (int i = 0; i < 128; i++)
    {
      u8g2.drawPixel( 64 + (32 - analogRead(0) / 8) * 2 + off, i );
    }

  } while ( u8g2.nextPage() );
  delay(random(500));

}

void makePixel()
{
  for (int k = 0; k < random(10); k++)
  {
    do {
      int h = random(0, 2);
      u8g2.setDrawColor(h);// Black
      for (int i = 0; i < random(2000); i++)
      {
        u8g2.drawPixel(random(128), random(128));
      }

    } while ( u8g2.nextPage() );
    delay(1000);
  }

}

void makePixelBIG()
{
  for (int k = 0; k < random(10); k++)
  {
    do {
      int h = random(0, 2);
      u8g2.setDrawColor(h);// Black
      int pix = random(5, 20);
      for (int i = 0; i < 150 / pix; i++)
      {
        u8g2.drawBox(int(150 / pix) * random(pix), int(150 / pix) * random(pix), pix, pix);
      }
      u8g2.setDrawColor(1 - h); // Black
      for (int i = 0; i < 150 / pix; i++)
      {
        u8g2.drawBox(int(150 / pix) * random(pix), int(150 / pix) * random(pix), pix, pix);
      }

    } while ( u8g2.nextPage() );
    delay(1000);
  }
}

/*
void testscrolltext(char thisString, int x, int y) {


  u8g2.firstPage();
  do {

    // draw the scrolling text at current offset
    x = offset;
    u8g2.setFont(u8g2_font_inb30_mr);    // set the target font
    do {                // repeated drawing of the scrolling text...
      u8g2.drawUTF8(x, y, &thisString);     // draw the scolling text
      x += width;           // add the pixel width of the scrolling text
    } while ( x < u8g2.getDisplayWidth() );   // draw again until the complete display is filled

    u8g2.setFont(u8g2_font_inb16_mr);   // draw the current pixel width
    u8g2.setCursor(0, 58);
    u8g2.print(width);          // this value must be lesser than 128 unless U8G2_16BIT is set

  } while ( u8g2.nextPage() );

   offset -= 1;            // scroll by one pixel
  if ( (u8g2_uint_t)offset < (u8g2_uint_t) - width )
    offset = 0;             // start over again

  delay(10);              // do some small delay
}
*/
